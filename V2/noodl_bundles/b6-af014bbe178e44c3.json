erRadius":{"value":100,"unit":"%"},"backgroundColor":"White","borderStyle":"solid","borderWidth":{"value":1,"unit":"px"},"width":{"value":40,"unit":"%"},"height":{"value":40,"unit":"%"},"flexDirection":"column","justifyContent":"center","alignItems":"center","alignX":"right","alignY":"center","paddingTop":{"value":2,"unit":"px"},"paddingLeft":{"value":2,"unit":"px"},"paddingBottom":{"value":2,"unit":"px"},"paddingRight":{"value":2,"unit":"px"},"marginTop":{"value":13,"unit":"px"}},"ports":[],"children":[{"id":"01de106e-4a63-1288-cf80-9aa113ed9751","type":"Text","parameters":{"position":"relative","sizeMode":"contentSize","text":"","textStyle":"Title Medium"},"ports":[],"children":[]}]}]}]},{"id":"d54875c5-d856-cdf3-7f05-852639e808d2","type":"JavaScriptFunction","parameters":{"functionScript":"/**\r\n * @role Récupère l'heure et la date du système, les formate, et les renvoie en sortie.\r\n * @output {string} Outputs.Hour - Heure au format \"HH:MM\".\r\n * @output {string} Outputs.DayName - Jour de la semaine (en français).\r\n * @output {string} Outputs.DayNumMonth - Numéro du jour et mois (ex: \"27 avril\").\r\n */\r\ntry {\r\n    const now = new Date();\r\n\r\n    // Format HH:MM avec zéro initial\r\n    const pad = n => n < 10 ? '0' + n : '' + n;\r\n    Outputs.Hour = pad(now.getHours()) + ':' + pad(now.getMinutes());\r\n\r\n    // Jours et mois en français\r\n    const jours = [\r\n        \"dimanche\", \"lundi\", \"mardi\", \"mercredi\", \"jeudi\", \"vendredi\", \"samedi\"\r\n    ];\r\n    const mois = [\r\n        \"janvier\", \"février\", \"mars\", \"avril\", \"mai\", \"juin\",\r\n        \"juillet\", \"août\", \"septembre\", \"octobre\", \"novembre\", \"décembre\"\r\n    ];\r\n\r\n    Outputs.DayName = jours[now.getDay()];\r\n    Outputs.DayNumMonth = now.getDate() + ' ' + mois[now.getMonth()];\r\n\r\n    Outputs.Success();\r\n} catch (err) {\r\n    Outputs.Failure(\"Erreur lors de la récupération de la date/heure : \" + err.message);\r\n}\r\n"},"ports":[{"n